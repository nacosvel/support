{
    "name": "nacosvel/support",
    "description": "The Nacosvel Components",
    "keywords": [
        "nacosvel",
        "support"
    ],
    "type": "library",
    "license": "MIT",
    "homepage": "https://github.com/nacosvel/support",
    "authors": [
        {
            "name": "jundayw",
            "email": "jundayw@126.com"
        }
    ],
    "require": {
        "php": "^8.0",
        "nacosvel/macroable": "^1.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^11.5"
    },
    "autoload": {
        "psr-4": {
            "Nacosvel\\Support\\": "src/"
        },
        "files": [
            "./src/helper.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "1.x-dev"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
